{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "amesdbtemp",
						"type": "DatasetReference"
					},
					"name": "amesdbtemp",
					"description": "se importa la tabla de hechos: amesdbtemp"
				},
				{
					"dataset": {
						"referenceName": "saleproperty",
						"type": "DatasetReference"
					},
					"name": "saleproperty",
					"description": "se importa la tabla saleproperty"
				},
				{
					"dataset": {
						"referenceName": "floordetail",
						"type": "DatasetReference"
					},
					"name": "floordetail",
					"description": "se importa la tabla floordetail"
				},
				{
					"dataset": {
						"referenceName": "mssubclass",
						"type": "DatasetReference"
					},
					"name": "mssubclass",
					"description": "se importa la tabla mssubclass"
				},
				{
					"dataset": {
						"referenceName": "mszoning",
						"type": "DatasetReference"
					},
					"name": "mszoning",
					"description": "se importa la tabla mszoning"
				},
				{
					"dataset": {
						"referenceName": "typequality",
						"type": "DatasetReference"
					},
					"name": "typequality",
					"description": "se importa la tabla typequality"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "output_data",
						"type": "DatasetReference"
					},
					"name": "sinkbdrelacional",
					"description": "Se genera el csv de salida."
				}
			],
			"transformations": [
				{
					"name": "join1",
					"description": "Se realiza el primer join entre las tablas: amesdbtemp y SaleProperties."
				},
				{
					"name": "join2",
					"description": "Se realiza el segundo join con la tabla floordetail."
				},
				{
					"name": "join3",
					"description": "Se realiza el tercer join con la tabla mssubclass."
				},
				{
					"name": "join4",
					"description": "Se realiza el cuarto join con la tabla mszoning."
				},
				{
					"name": "join5",
					"description": "Se realiza el quinto join con la tabla typequality."
				},
				{
					"name": "aggregate2",
					"description": "Como cada propiedad puede tener varios pisos, es necesario consolidar totales por propiedad."
				},
				{
					"name": "derivedColumn1",
					"description": "Se crea dos nueva columnas a partir de sale date."
				},
				{
					"name": "select3",
					"description": "Se agrega solo las columnas que se requieren segÃºn el archivo de salida y se eliminan las columnas redundantes"
				},
				{
					"name": "select1",
					"description": "Eliminar las columnas redundantes"
				},
				{
					"name": "select2",
					"description": "Eliminar las columnas redundantes"
				},
				{
					"name": "select4"
				},
				{
					"name": "join6",
					"description": "Se realiza el sexto join con la tabla typequality."
				},
				{
					"name": "select5",
					"description": "Renombrar las columnas del join de typequality"
				},
				{
					"name": "select6",
					"description": "Renombrar las columnas del join de typequality"
				},
				{
					"name": "join7",
					"description": "Se realiza el septimo join con la tabla typequality."
				},
				{
					"name": "select7",
					"description": "Renombrar las columnas del join de typequality"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Exter Cond} as long,",
				"          {Exter Qual} as long,",
				"          {Heating QC} as long,",
				"          {Kitchen Qual} as long,",
				"          {MS Zoning} as long,",
				"          pid as long,",
				"          {Roof Style} as string,",
				"          {Roof Matl} as string,",
				"          {Exterior 1st} as string,",
				"          {Exterior 2nd} as string,",
				"          {Mas Vnr Type} as string,",
				"          {Mas Vnr Area} as long,",
				"          foundation as string,",
				"          heating as string,",
				"          {Central Air} as boolean,",
				"          electrical as string,",
				"          {1st Flr SF} as long,",
				"          {2nd Flr SF} as long,",
				"          {Low Qual Fin SF} as long,",
				"          {Kitchen AbvGr} as long,",
				"          {TotRms AbvGrd} as long,",
				"          functional as string,",
				"          fireplaces as long,",
				"          {Fireplace Qu} as string,",
				"          {Paved Drive} as string,",
				"          {Wood Deck SF} as long,",
				"          {Open Porch SF} as long,",
				"          {Enclosed Porch} as long,",
				"          {3Ssn Porch} as long,",
				"          {Screen Porch} as long,",
				"          fence as string,",
				"          {MS SubClass} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     partitionBy('hash', 1)) ~> amesdbtemp",
				"source(output(",
				"          pid as long,",
				"          {Sale Date} as timestamp,",
				"          {Sale Type} as string,",
				"          {Sale Condition} as string,",
				"          saleprice as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> saleproperty",
				"source(output(",
				"          pid as long,",
				"          Floor as long,",
				"          bedrooms as long,",
				"          {Full Bath} as long,",
				"          {Half Bath} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> floordetail",
				"source(output(",
				"          description as string,",
				"          code as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> mssubclass",
				"source(output(",
				"          id as long,",
				"          description as string,",
				"          code as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> mszoning",
				"source(output(",
				"          id as long,",
				"          code as string,",
				"          description as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> typequality",
				"amesdbtemp, saleproperty join(amesdbtemp@pid == saleproperty@pid,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join1",
				"join1, aggregate2 join(amesdbtemp@pid == aggregate2@pid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join2",
				"select1, mssubclass join({MS SubClass} == code,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join3",
				"join3, mszoning join({MS Zoning} == id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join4",
				"select2, select4 join({Exter Qual} == id_typequality,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join5",
				"floordetail aggregate(groupBy(pid),",
				"     FullBath = sum({Full Bath}),",
				"          HalfBath = sum({Half Bath}),",
				"          Bedrooms = sum(bedrooms),",
				"     partitionBy('hash', 1)) ~> aggregate2",
				"select7 derive(MoSold = month({Sale Date}),",
				"          YrSold = year({Sale Date}),",
				"     partitionBy('hash', 1)) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          PID,",
				"          {MS SubClass},",
				"          {MS Zoning},",
				"          {Roof Style},",
				"          {Roof Matl},",
				"          {Exterior 1st},",
				"          {Exterior 2nd},",
				"          {Mas Vnr Type},",
				"          {Mas Vnr Area},",
				"          {Exter Qual} = {Heating QC},",
				"          {Exter Cond},",
				"          foundation,",
				"          heating,",
				"          {Heating QC},",
				"          {Central Air},",
				"          electrical,",
				"          {1st Flr SF},",
				"          {2nd Flr SF},",
				"          {Low Qual Fin SF},",
				"          FullBath,",
				"          HalfBath,",
				"          Bedrooms,",
				"          {Kitchen AbvGr},",
				"          {Kitchen Qual},",
				"          {TotRms AbvGrd},",
				"          functional,",
				"          fireplaces,",
				"          {Fireplace Qu},",
				"          {Paved Drive},",
				"          {Wood Deck SF},",
				"          {Open Porch SF},",
				"          {Enclosed Porch},",
				"          {3Ssn Porch},",
				"          {Screen Porch},",
				"          fence,",
				"          MoSold,",
				"          YrSold,",
				"          {Sale Type},",
				"          {Sale Condition},",
				"          saleprice",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"join2 select(mapColumn(",
				"          {Exter Cond},",
				"          {Exter Qual},",
				"          {Heating QC},",
				"          {Kitchen Qual},",
				"          {MS Zoning},",
				"          pid = amesdbtemp@pid,",
				"          {Roof Style},",
				"          {Roof Matl},",
				"          {Exterior 1st},",
				"          {Exterior 2nd},",
				"          {Mas Vnr Type},",
				"          {Mas Vnr Area},",
				"          foundation,",
				"          heating,",
				"          {Central Air},",
				"          electrical,",
				"          {1st Flr SF},",
				"          {2nd Flr SF},",
				"          {Low Qual Fin SF},",
				"          {Kitchen AbvGr},",
				"          {TotRms AbvGrd},",
				"          functional,",
				"          fireplaces,",
				"          {Fireplace Qu},",
				"          {Paved Drive},",
				"          {Wood Deck SF},",
				"          {Open Porch SF},",
				"          {Enclosed Porch},",
				"          {3Ssn Porch},",
				"          {Screen Porch},",
				"          fence,",
				"          {MS SubClass},",
				"          pid = saleproperty@pid,",
				"          {Sale Date},",
				"          {Sale Type},",
				"          {Sale Condition},",
				"          saleprice,",
				"          pid = aggregate2@pid,",
				"          FullBath,",
				"          HalfBath,",
				"          Bedrooms",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"join4 select(mapColumn(",
				"          PID = pid,",
				"          {MS SubClass},",
				"          {MS Zoning} = mszoning@code,",
				"          {Roof Style},",
				"          {Roof Matl},",
				"          {Exterior 1st},",
				"          {Exterior 2nd},",
				"          {Mas Vnr Area},",
				"          {Exter Qual},",
				"          {Exter Cond},",
				"          foundation,",
				"          {Heating QC},",
				"          {Kitchen Qual},",
				"          {Mas Vnr Type},",
				"          heating,",
				"          {Central Air},",
				"          electrical,",
				"          {1st Flr SF},",
				"          {2nd Flr SF},",
				"          {Low Qual Fin SF},",
				"          {Kitchen AbvGr},",
				"          {TotRms AbvGrd},",
				"          functional,",
				"          fireplaces,",
				"          {Fireplace Qu},",
				"          {Paved Drive},",
				"          {Wood Deck SF},",
				"          {Open Porch SF},",
				"          {Enclosed Porch},",
				"          {3Ssn Porch},",
				"          {Screen Porch},",
				"          fence,",
				"          {Sale Date},",
				"          {Sale Type},",
				"          {Sale Condition},",
				"          saleprice,",
				"          FullBath,",
				"          HalfBath,",
				"          Bedrooms,",
				"          description = mssubclass@description,",
				"          code = mssubclass@code,",
				"          id,",
				"          description = mszoning@description",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"typequality select(mapColumn(",
				"          id_typequality = id,",
				"          code_typequality = code,",
				"          description_typequality = description",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select4",
				"select5, select4 join({Exter Cond} == select4@id_typequality,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join6",
				"join5 select(mapColumn(",
				"          PID,",
				"          {MS SubClass},",
				"          {MS Zoning},",
				"          {Roof Style},",
				"          {Roof Matl},",
				"          {Exterior 1st},",
				"          {Exterior 2nd},",
				"          {Mas Vnr Area},",
				"          {Exter Cond},",
				"          foundation,",
				"          {Heating QC},",
				"          {Kitchen Qual},",
				"          {Mas Vnr Type},",
				"          heating,",
				"          {Central Air},",
				"          electrical,",
				"          {1st Flr SF},",
				"          {2nd Flr SF},",
				"          {Low Qual Fin SF},",
				"          {Kitchen AbvGr},",
				"          {TotRms AbvGrd},",
				"          functional,",
				"          fireplaces,",
				"          {Fireplace Qu},",
				"          {Paved Drive},",
				"          {Wood Deck SF},",
				"          {Open Porch SF},",
				"          {Enclosed Porch},",
				"          {3Ssn Porch},",
				"          {Screen Porch},",
				"          fence,",
				"          {Sale Date},",
				"          {Sale Type},",
				"          {Sale Condition},",
				"          saleprice,",
				"          FullBath,",
				"          HalfBath,",
				"          Bedrooms,",
				"          description,",
				"          code,",
				"          id,",
				"          id_typequality,",
				"          {Exter Qual} = code_typequality",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select5",
				"join6 select(mapColumn(",
				"          PID,",
				"          {MS SubClass},",
				"          {MS Zoning},",
				"          {Roof Style},",
				"          {Roof Matl},",
				"          {Exterior 1st},",
				"          {Exterior 2nd},",
				"          {Mas Vnr Area},",
				"          foundation,",
				"          {Heating QC},",
				"          {Kitchen Qual},",
				"          {Mas Vnr Type},",
				"          heating,",
				"          {Central Air},",
				"          electrical,",
				"          {1st Flr SF},",
				"          {2nd Flr SF},",
				"          {Low Qual Fin SF},",
				"          {Kitchen AbvGr},",
				"          {TotRms AbvGrd},",
				"          functional,",
				"          fireplaces,",
				"          {Fireplace Qu},",
				"          {Paved Drive},",
				"          {Wood Deck SF},",
				"          {Open Porch SF},",
				"          {Enclosed Porch},",
				"          {3Ssn Porch},",
				"          {Screen Porch},",
				"          fence,",
				"          {Sale Date},",
				"          {Sale Type},",
				"          {Sale Condition},",
				"          saleprice,",
				"          FullBath,",
				"          HalfBath,",
				"          Bedrooms,",
				"          description,",
				"          code,",
				"          id,",
				"          {Exter Qual},",
				"          id_typequality = select4@id_typequality,",
				"          {Exter Cond} = code_typequality",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select6",
				"select6, select4 join({Heating QC} == select4@id_typequality,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join7",
				"join7 select(mapColumn(",
				"          PID,",
				"          {MS SubClass},",
				"          {MS Zoning},",
				"          {Roof Style},",
				"          {Roof Matl},",
				"          {Exterior 1st},",
				"          {Exterior 2nd},",
				"          {Mas Vnr Area},",
				"          {Exter Cond},",
				"          foundation,",
				"          {Kitchen Qual},",
				"          {Mas Vnr Type},",
				"          heating,",
				"          {Central Air},",
				"          electrical,",
				"          {1st Flr SF},",
				"          {2nd Flr SF},",
