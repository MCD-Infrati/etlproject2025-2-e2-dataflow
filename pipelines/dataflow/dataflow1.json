{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "amesdbtemp",
						"type": "DatasetReference"
					},
					"name": "amesdbtemp",
					"description": "se importa la tabla de hechos: amesdbtemp"
				},
				{
					"dataset": {
						"referenceName": "saleproperty",
						"type": "DatasetReference"
					},
					"name": "saleproperty",
					"description": "se importa la tabla saleproperty"
				},
				{
					"dataset": {
						"referenceName": "floordetail",
						"type": "DatasetReference"
					},
					"name": "floordetail",
					"description": "se importa la tabla floordetail"
				},
				{
					"dataset": {
						"referenceName": "mssubclass",
						"type": "DatasetReference"
					},
					"name": "mssubclass",
					"description": "se importa la tabla mssubclass"
				},
				{
					"dataset": {
						"referenceName": "mszoning",
						"type": "DatasetReference"
					},
					"name": "mszoning",
					"description": "se importa la tabla mszoning"
				},
				{
					"dataset": {
						"referenceName": "typequality",
						"type": "DatasetReference"
					},
					"name": "typequality",
					"description": "se importa la tabla typequality"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "join1",
					"description": "Se realiza el primer join entre las tablas: amesdbtemp y SaleProperties."
				},
				{
					"name": "join2",
					"description": "Se realiza el segundo join con la tabla floordetail."
				},
				{
					"name": "join3",
					"description": "Se realiza el tercer join con la tabla mssubclass."
				},
				{
					"name": "join4",
					"description": "Se realiza el cuarto join con la tabla mszoning."
				},
				{
					"name": "join5",
					"description": "Se realiza el quinto join con la tabla typequality."
				},
				{
					"name": "aggregate2",
					"description": "AgregaciÃ³n de BaÃ±os y Habitaciones"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Exter Cond} as long,",
				"          {Exter Qual} as long,",
				"          {Heating QC} as long,",
				"          {Kitchen Qual} as long,",
				"          {MS Zoning} as long,",
				"          pid as long,",
				"          {Roof Style} as string,",
				"          {Roof Matl} as string,",
				"          {Exterior 1st} as string,",
				"          {Exterior 2nd} as string,",
				"          {Mas Vnr Type} as string,",
				"          {Mas Vnr Area} as long,",
				"          foundation as string,",
				"          heating as string,",
				"          {Central Air} as boolean,",
				"          electrical as string,",
				"          {1st Flr SF} as long,",
				"          {2nd Flr SF} as long,",
				"          {Low Qual Fin SF} as long,",
				"          {Kitchen AbvGr} as long,",
				"          {TotRms AbvGrd} as long,",
				"          functional as string,",
				"          fireplaces as long,",
				"          {Fireplace Qu} as string,",
				"          {Paved Drive} as string,",
				"          {Wood Deck SF} as long,",
				"          {Open Porch SF} as long,",
				"          {Enclosed Porch} as long,",
				"          {3Ssn Porch} as long,",
				"          {Screen Porch} as long,",
				"          fence as string,",
				"          {MS SubClass} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     partitionBy('hash', 1)) ~> amesdbtemp",
				"source(output(",
				"          pid as long,",
				"          {Sale Date} as timestamp,",
				"          {Sale Type} as string,",
				"          {Sale Condition} as string,",
				"          saleprice as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> saleproperty",
				"source(output(",
				"          pid as long,",
				"          Floor as long,",
				"          bedrooms as long,",
				"          {Full Bath} as long,",
				"          {Half Bath} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> floordetail",
				"source(output(",
				"          description as string,",
				"          code as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> mssubclass",
				"source(output(",
				"          id as long,",
				"          description as string,",
				"          code as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> mszoning",
				"source(output(",
				"          id as long,",
				"          code as string,",
				"          description as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> typequality",
				"amesdbtemp, saleproperty join(amesdbtemp@pid == saleproperty@pid,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join1",
				"join1, aggregate2 join(amesdbtemp@pid == aggregate2@pid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join2",
				"join2, mssubclass join({MS SubClass} == code,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join3",
				"join3, mszoning join({MS Zoning} == id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join4",
				"join4, typequality join({Exter Qual} == typequality@id",
				"     && {Exter Cond} == typequality@id",
				"     && {Heating QC} == typequality@id",
				"     && {Kitchen Qual} == typequality@id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join5",
				"floordetail aggregate(groupBy(pid,",
				"          Floor,",
				"          bedrooms,",
				"          {Full Bath},",
				"          {Half Bath}),",
				"     TotalFullBath = sum({Full Bath}),",
				"          TotalHalfBath = sum({Half Bath}),",
				"          TotalBedrooms = sum(bedrooms),",
				"     partitionBy('hash', 1)) ~> aggregate2",
				"join5 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}