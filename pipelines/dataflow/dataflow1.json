				"          {Paved Drive},",
				"          {Wood Deck SF},",
				"          {Open Porch SF},",
				"          {Enclosed Porch},",
				"          {3Ssn Porch},",
				"          {Screen Porch},",
				"          fence,",
				"          {Sale Date},",
				"          {Sale Type},",
				"          {Sale Condition},",
				"          saleprice,",
				"          FullBath,",
