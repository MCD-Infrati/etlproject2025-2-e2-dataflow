{
	"name": "dataflowCSV",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AmesPropertyCSV",
						"type": "DatasetReference"
					},
					"name": "AmsPropertyCSV"
				},
				{
					"dataset": {
						"referenceName": "neighborhood",
						"type": "DatasetReference"
					},
					"name": "neighborhood"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "Rename",
					"description": "Renaming AmsPropertyCSV to columns YearBuilt, YearRemodAdd'"
				},
				{
					"name": "Reemplazafecha",
					"description": "Creating/updating the columns YearRemodAdd'"
				},
				{
					"name": "join1Neigborhood"
				},
				{
					"name": "ActnombreNeighborhood",
					"description": "Retirar espacikos columnas"
				},
				{
					"name": "Tranlotshape",
					"description": "Convertir LotShape en string"
				},
				{
					"name": "derivedColumn2",
					"description": "Creating/upda"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PID as string,",
				"          {Lot Frontage} as string,",
				"          {Lot Area} as string,",
				"          Street as string,",
				"          Alley as string,",
				"          {Lot Shape} as string,",
				"          {Land Contour} as string,",
				"          Utilities as string,",
				"          {Lot Config} as string,",
				"          {Land Slope} as string,",
				"          Neighborhood as string,",
				"          {Condition 1} as string,",
				"          {Condition 2} as string,",
				"          {Bldg Type} as string,",
				"          {House Style} as string,",
				"          {Overall Qual} as string,",
				"          {Overall Cond} as string,",
				"          {Year Built} as integer,",
				"          {Year Remod/Add} as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> AmsPropertyCSV",
				"source(output(",
				"          code as string,",
				"          description as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> neighborhood",
				"AmsPropertyCSV select(mapColumn(",
				"          PID,",
				"          {Lot Frontage},",
				"          {Lot Area},",
				"          Street,",
				"          Alley,",
				"          {Lot Shape},",
				"          {Land Contour},",
				"          Utilities,",
				"          {Lot Config},",
				"          {Land Slope},",
				"          Neighborhood,",
				"          {Condition 1},",
				"          {Condition 2},",
				"          {Bldg Type},",
				"          {House Style},",
				"          {Overall Qual},",
				"          {Overall Cond},",
				"          YearBuilt = {Year Built},",
				"          YearRemodAdd = {Year Remod/Add}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Rename",
				"Rename derive(YearRemodAdd = iif(isNull(toInteger(YearRemodAdd)), YearBuilt, toInteger(YearRemodAdd))) ~> Reemplazafecha",
				"Reemplazafecha, neighborhood join(Neighborhood == description,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1Neigborhood",
				"join1Neigborhood select(mapColumn(",
				"          PID,",
				"          {Lot Frontage},",
				"          {Lot Area},",
				"          Street,",
				"          Alley,",
				"          LotShape = {Lot Shape},",
				"          {Land Contour},",
				"          Utilities,",
				"          {Lot Config},",
				"          {Land Slope},",
				"          Neighborhood,",
				"          Neighborhoodcode = code,",
				"          Condition1 = {Condition 1},",
				"          Condition2 = {Condition 2},",
				"          {Bldg Type},",
				"          {House Style},",
				"          {Overall Qual},",
				"          {Overall Cond},",
				"          YearBuilt,",
				"          YearRemodAdd",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ActnombreNeighborhood",
				"ActnombreNeighborhood derive(LotShape = case(     toString(LotShape) == 'Regular', 'Reg',     toString(LotShape) == 'Slightly irregular', 'IR1',     toString(LotShape) == 'Moderately Irregular', 'IR2',     toString(LotShape) == 'Irregular', 'IR3',     'Unknown' )) ~> Tranlotshape",
				"Tranlotshape derive(Conditon1name = case(\r",
				"  Condition1 == 'Artery','Adjacent to arterial street',\r",
				"  Condition1 == 'Feedr','Adjacent to feeder street',\r",
				"  Condition1 == 'Norm',   'Normal',\r",
				"  Condition1 == 'RRNn',   'Within 200 of North-South Railroad ',\r",
				"  Condition1 == 'RRAn',   'Within 200 of North-South Railroad ',\r",
				"  Condition1 == 'PosN',   'Near positive off-site feature--park greenbelt etc. ',\r",
				"  Condition1 == 'PosA',   'Adjacent to postive off-site feature ',\r",
				"  Condition1 == 'RRNe',   'Within 200 of East-West Railroad  ',\r",
				"  Condition1 == 'RRAe',   'Adjacent to East-West Railroad    ',\r",
				" \r",
				"  'Unknown'\r",
				")\r",
				") ~> derivedColumn2"
			]
		}
	}
}