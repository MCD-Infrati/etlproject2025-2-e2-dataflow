{
	"name": "dataflowCSV",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AmesPropertyCSV",
						"type": "DatasetReference"
					},
					"name": "AmsPropertyCSV"
				},
				{
					"dataset": {
						"referenceName": "neighborhood",
						"type": "DatasetReference"
					},
					"name": "neighborhood"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "Rename",
					"description": "Renaming AmsPropertyCSV to columns YearBuilt, YearRemodAdd'"
				},
				{
					"name": "Reemplazafecha",
					"description": "Creating/updating the columns YearRemodAdd'"
				},
				{
					"name": "join1"
				},
				{
					"name": "select1",
					"description": "Renaming join1 to '"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PID as string,",
				"          {Lot Frontage} as string,",
				"          {Lot Area} as string,",
				"          Street as string,",
				"          Alley as string,",
				"          {Lot Shape} as string,",
				"          {Land Contour} as string,",
				"          Utilities as string,",
				"          {Lot Config} as string,",
				"          {Land Slope} as string,",
				"          Neighborhood as string,",
				"          {Condition 1} as string,",
				"          {Condition 2} as string,",
				"          {Bldg Type} as string,",
				"          {House Style} as string,",
				"          {Overall Qual} as string,",
				"          {Overall Cond} as string,",
				"          {Year Built} as integer,",
				"          {Year Remod/Add} as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> AmsPropertyCSV",
				"source(output(",
				"          code as string,",
				"          description as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> neighborhood",
				"AmsPropertyCSV select(mapColumn(",
				"          PID,",
				"          {Lot Frontage},",
				"          {Lot Area},",
				"          Street,",
				"          Alley,",
				"          {Lot Shape},",
				"          {Land Contour},",
				"          Utilities,",
				"          {Lot Config},",
				"          {Land Slope},",
				"          Neighborhood,",
				"          {Condition 1},",
				"          {Condition 2},",
				"          {Bldg Type},",
				"          {House Style},",
				"          {Overall Qual},",
				"          {Overall Cond},",
				"          YearBuilt = {Year Built},",
				"          YearRemodAdd = {Year Remod/Add}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Rename",
				"Rename derive(YearRemodAdd = iif(isNull(toInteger(YearRemodAdd)), YearBuilt, toInteger(YearRemodAdd))) ~> Reemplazafecha",
				"Reemplazafecha, neighborhood join(Neighborhood == description,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          PID,",
				"          {Lot Frontage},",
				"          {Lot Area},",
				"          Street,",
				"          Alley,",
				"          {Lot Shape},",
				"          {Land Contour},",
				"          Utilities,",
				"          {Lot Config},",
				"          {Land Slope},",
				"          Neighborhood,",
				"          {Condition 1},",
				"          {Condition 2},",
				"          {Bldg Type},",
				"          {House Style},",
				"          {Overall Qual},",
				"          {Overall Cond},",
				"          YearBuilt,",
				"          YearRemodAdd,",
				"          code,",
				"          description",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}