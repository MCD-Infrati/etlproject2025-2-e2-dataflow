{
	"name": "dataflow_transformaciones",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "input_sql",
						"type": "DatasetReference"
					},
					"name": "fullamesdbtemp",
					"description": "Data obtenida de la consulta sql de Neon."
				},
				{
					"dataset": {
						"referenceName": "floordetail",
						"type": "DatasetReference"
					},
					"name": "floordetail",
					"description": "se importa la tabla floordetail"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "output_data",
						"type": "DatasetReference"
					},
					"name": "sinkbdrelacional",
					"description": "Se genera el csv de salida.",
					"rejectedDataLinkedService": {
						"referenceName": "BlobProyectoFinal_Bdrelacional",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "Totalespropiedad",
					"description": "Como cada propiedad puede tener varios pisos, es necesario consolidar totales por propiedad."
				},
				{
					"name": "Seleccion1",
					"description": "Eliminar las columnas redundantes"
				},
				{
					"name": "join1",
					"description": "Se realiza un join con el resultado de la  tabla floordetail."
				},
				{
					"name": "selectFinal",
					"description": "Se agrega solo las columnas que se requieren segÃºn el archivo de salida y se eliminan las columnas redundantes"
				},
				{
					"name": "Nuevascolumnas"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Exter Cond} as integer,",
				"          {Exter Qual} as integer,",
				"          {Heating QC} as integer,",
				"          {Kitchen Qual} as integer,",
				"          {MS Zoning} as integer,",
				"          pid as integer,",
				"          {Roof Style} as string,",
				"          {Roof Matl} as string,",
				"          {Exterior 1st} as string,",
				"          {Exterior 2nd} as string,",
				"          {Mas Vnr Type} as string,",
				"          {Mas Vnr Area} as integer,",
				"          foundation as string,",
				"          heating as string,",
				"          {Central Air} as boolean,",
				"          electrical as string,",
				"          {1st Flr SF} as integer,",
				"          {2nd Flr SF} as integer,",
				"          {Low Qual Fin SF} as integer,",
				"          {Kitchen AbvGr} as integer,",
				"          {TotRms AbvGrd} as integer,",
				"          functional as string,",
				"          fireplaces as integer,",
				"          {Fireplace Qu} as string,",
				"          {Paved Drive} as string,",
				"          {Wood Deck SF} as integer,",
				"          {Open Porch SF} as integer,",
				"          {Enclosed Porch} as integer,",
				"          {3Ssn Porch} as integer,",
				"          {Screen Porch} as integer,",
				"          fence as string,",
				"          {MS SubClass} as integer,",
				"          {Sale Date} as date 'yyyy-MM-dd',",
				"          {Sale Type} as string,",
				"          {Sale Condition} as string,",
				"          saleprice as integer,",
				"          {MS SubClass1} as integer,",
				"          {MS Zoning1} as string,",
				"          {Kitchen Qual1} as string,",
				"          {Exter Qual1} as string,",
				"          {Exter Cond1} as string,",
				"          {Heating QC1} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     dateFormats: ['yyyy/MM/dd'],",
				"     timestampFormats: ['yyyy.MM.dd HH:mm:ss'],",
				"     preferredIntegralType: 'integer',",
				"     preferredFractionalType: 'float',",
				"     partitionBy('hash', 1)) ~> fullamesdbtemp",
				"source(output(",
				"          pid as long,",
				"          Floor as long,",
				"          bedrooms as long,",
				"          {Full Bath} as long,",
				"          {Half Bath} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> floordetail",
				"floordetail aggregate(groupBy(pid),",
				"     FullBath = sum({Full Bath}),",
				"          HalfBath = sum({Half Bath}),",
				"          {Bedrooms  AbvGr} = sum(bedrooms),",
				"     partitionBy('hash', 1)) ~> Totalespropiedad",
				"fullamesdbtemp select(mapColumn(",
				"          pid,",
				"          {Roof Style},",
				"          {Roof Matl},",
				"          {Exterior 1st},",
				"          {Exterior 2nd},",
				"          {Mas Vnr Type},",
				"          {Mas Vnr Area},",
				"          foundation,",
				"          heating,",
				"          {Central Air},",
				"          electrical,",
				"          {1st Flr SF},",
				"          {2nd Flr SF},",
				"          {Low Qual Fin SF},",
				"          {Kitchen AbvGr},",
				"          {TotRms AbvGrd},",
				"          functional,",
				"          fireplaces,",
				"          {Fireplace Qu},",
				"          {Paved Drive},",
				"          {Wood Deck SF},",
				"          {Open Porch SF},",
				"          {Enclosed Porch},",
				"          {3Ssn Porch},",
				"          {Screen Porch},",
				"          fence,",
				"          {Sale Date},",
				"          {Sale Type},",
				"          {Sale Condition},",
				"          saleprice,",
				"          {MS SubClass} = {MS SubClass1},",
				"          {MS Zoning} = {MS Zoning1},",
				"          {Kitchen Qual} = {Kitchen Qual1},",
				"          {Exter Qual} = {Exter Qual1},",
				"          {Exter Cond} = {Exter Cond1},",
				"          {Heating QC} = {Heating QC1}",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Seleccion1",
				"Seleccion1, Totalespropiedad join(Seleccion1@pid == Totalespropiedad@pid,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> join1",
				"Nuevascolumnas select(mapColumn(",
				"          PID = Seleccion1@pid,",
				"          {MS SubClass},",
				"          {MS Zoning},",
				"          {Roof Style},",
				"          {Roof Matl},",
				"          {Exterior 1st},",
				"          {Exterior 2nd},",
				"          {Mas Vnr Type},",
				"          {Mas Vnr Area},",
				"          {Exter Qual},",
				"          {Exter Cond},",
				"          Foundation = foundation,",
				"          Heating = heating,",
				"          {Heating QC},",
				"          {Central Air},",
				"          Electrical = electrical,",
				"          {1st Flr SF},",
				"          {2nd Flr SF},",
				"          {Low Qual Fin SF},",
				"          {Gr Liv Area},",
				"          FullBath,",
				"          HalfBath,",
				"          {Bedrooms  AbvGr},",
				"          {Kitchen AbvGr},",
				"          {Kitchen Qual},",
				"          {TotRms AbvGrd},",
				"          Functional = functional,",
				"          fireplaces,",
				"          {Fireplace Qu},",
				"          {Paved Drive},",
				"          {Wood Deck SF},",
				"          {Open Porch SF},",
				"          {Enclosed Porch},",
				"          {3Ssn Porch},",
				"          {Screen Porch},",
				"          Fence = fence,",
				"          MoSold,",
				"          YrSold,",
				"          {Sale Type},",
				"          {Sale Condition},",
				"          saleprice",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectFinal",
				"join1 derive({Gr Liv Area} = {1st Flr SF}+{2nd Flr SF}+{Low Qual Fin SF},",
				"          MoSold = month({Sale Date}),",
				"          YrSold = year({Sale Date}),",
				"          {Central Air} = iif({Central Air} == true(), \"Y\", \"N\"),",
				"     partitionBy('hash', 1)) ~> Nuevascolumnas",
				"selectFinal sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['bdrelacional_final.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> sinkbdrelacional"
			]
		}
	}
}