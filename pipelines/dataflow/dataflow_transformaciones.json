{
	"name": "dataflow_transformaciones",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "input_neon",
						"type": "DatasetReference"
					},
					"name": "fullamesdbtemp",
					"description": "Data obtenida de la consulta sql de Neon."
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "output_data",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {Exter Cond} as short,",
				"          {Exter Qual} as short,",
				"          {Heating QC} as short,",
				"          {Kitchen Qual} as short,",
				"          {MS Zoning} as short,",
				"          pid as integer,",
				"          {Roof Style} as string,",
				"          {Roof Matl} as string,",
				"          {Exterior 1st} as string,",
				"          {Exterior 2nd} as string,",
				"          {Mas Vnr Type} as string,",
				"          {Mas Vnr Area} as short,",
				"          foundation as string,",
				"          heating as string,",
				"          {Central Air} as boolean,",
				"          electrical as string,",
				"          {1st Flr SF} as short,",
				"          {2nd Flr SF} as short,",
				"          {Low Qual Fin SF} as short,",
				"          {Kitchen AbvGr} as short,",
				"          {TotRms AbvGrd} as short,",
				"          functional as string,",
				"          fireplaces as short,",
				"          {Fireplace Qu} as string,",
				"          {Paved Drive} as string,",
				"          {Wood Deck SF} as short,",
				"          {Open Porch SF} as short,",
				"          {Enclosed Porch} as short,",
				"          {3Ssn Porch} as short,",
				"          {Screen Porch} as short,",
				"          fence as string,",
				"          {MS SubClass} as short,",
				"          {Sale Date} as string,",
				"          {Sale Type} as string,",
				"          {Sale Condition} as string,",
				"          saleprice as integer,",
				"          {MS SubClass1} as short,",
				"          {MS Zoning1} as string,",
				"          {Kitchen Qual1} as string,",
				"          {Exter Qual1} as string,",
				"          {Exter Cond1} as string,",
				"          {Heating QC1} as string,",
				"          Floor as boolean,",
				"          bedrooms as short,",
				"          {Full Bath} as short,",
				"          {Half Bath} as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     dateFormats: ['yyyy/MM/dd'],",
				"     timestampFormats: ['yyyy.MM.dd HH:mm:ss'],",
				"     preferredIntegralType: 'integer',",
				"     preferredFractionalType: 'float',",
				"     partitionBy('hash', 1)) ~> fullamesdbtemp",
				"fullamesdbtemp sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1) ~> sink1"
			]
		}
	}
}