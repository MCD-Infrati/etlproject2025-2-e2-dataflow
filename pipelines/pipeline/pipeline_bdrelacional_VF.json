				"typeProperties": {
					"source": {
						"type": "AzurePostgreSqlSource",
						"query": "SELECT\r\n  a.*,\r\n  s.\"Sale Date\",\r\n  s.\"Sale Type\",\r\n  s.\"Sale Condition\",\r\n  s.saleprice,\r\n  m.code AS \"MS SubClass1\",\r\n  z.code AS \"MS Zoning1\",\r\n  tq_kitchen.code AS \"Kitchen Qual1\",\r\n  tq_exterqual.code AS \"Exter Qual1\",\r\n  tq_extercond.code AS \"Exter Cond1\",\r\n  tq_heatingqc.code AS \"Heating QC1\"\r\nFROM amesdbtemp a\r\nINNER JOIN saleproperty s\r\n    ON a.pid = s.pid\r\nINNER JOIN mssubclass m\r\n    ON a.\"MS SubClass\" = m.code\r\nINNER JOIN mszoning z\r\n    ON a.\"MS Zoning\" = z.id\r\nINNER JOIN typequality tq_kitchen\r\n    ON a.\"Kitchen Qual\" = tq_kitchen.id\r\nINNER JOIN typequality tq_exterqual\r\n    ON a.\"Exter Qual\" = tq_exterqual.id\r\nINNER JOIN typequality tq_extercond\r\n    ON a.\"Exter Cond\" = tq_extercond.id\r\nINNER JOIN typequality tq_heatingqc\r\n    ON a.\"Heating QC\" = tq_heatingqc.id",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
